# Redis - Upstash
# REST API credentials - used to build TCP connection for Celery and pub/sub
UPSTASH_REDIS_REST_URL=https://your-endpoint.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_token
UPSTASH_REDIS_PORT=6379

# Database
SUPABASE_URL=...
SUPABASE_KEY=...

# Email
EMAIL_PASSWORD=...

# Frontend URLs (json array)
ALLOWED_ORIGINS=[...]

# Proxies (json array format)
PROXY_STR=["http://proxy1.example.com:port","http://proxy2.example.com:port"]
PROXY_USERNAME=your_proxy_username
PROXY_PASSWORD=your_proxy_password